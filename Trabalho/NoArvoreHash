package Trabalho;

public class NoArvoreHash {
	private ClienteHash info;
	private NoArvoreHash dir, esq;
	
	public NoArvoreHash (ClienteHash _info){
		this.info = _info;
	}
	public NoArvoreHash() {
		
	}
	
	
	public NoArvoreHash getDir() {
		return dir;
	}
	public void setDir(NoArvoreHash dir) {
		this.dir = dir;
	}
	public NoArvoreHash getEsq() {
		return esq;
	}
	public void setEsq(NoArvoreHash esq) {
		this.esq = esq;
	}
	public ClienteHash getInfo() {
		return info;
	}
	public void setInfo(ClienteHash novo) {
		this.info = novo;
	}

}
