  
package Trabalho;

public class ClienteHash {
	long agencia = 0;
	long numero= 0;
	double saldo= 0;
	long cpf= 0;
	
	public ClienteHash(long agencia, long numero, double saldo, Long cpf) {
		this.agencia = agencia;
		this.numero = numero;
		this.saldo = saldo;
		this.cpf = cpf;
	}
	
	public ClienteHash(String str) {
		String[] temp = str.split(";");
		this.agencia = Long.parseLong(temp[0]);
		this.numero = Long.parseLong(temp[1]);
		this.saldo = Double.parseDouble(temp[2]);
		this.cpf = Long.parseLong(temp[3]);
	}
	public ClienteHash() {
		agencia = 0;
		numero = 0;
		saldo = 0;
		cpf = 0;
	}

	
	public long getAgencia() {
		return agencia;
	}

	public void setAgencia(long agencia) {
		this.agencia = agencia;
	}

	public long getNumero() {
		return numero;
	}

	public void setNumero(long numero) {
		this.numero = numero;
	}

	public double getSaldo() {
		return saldo;
	}

	public void setSaldo(double saldo) {
		this.saldo = saldo;
	}

	//Retorna o valor do Cpf (long)
	public long getChave() {
		return cpf;
	}
	
	//Retorna o endere√ßo do objeto Cpf
	public Long getCpf() {
		return cpf;
	}
	
	public long getNum() {
		return numero;
	}
	

	public void setCpf(Long cpf) {
		this.cpf = cpf;
	}
	

	@Override
	public String toString() {
		return "Cliente:\n"  
				+ "CPF: " + cpf + "\nAgencia: "+agencia
				+"\nConta: "+numero+"\nSaldo: "+saldo;
	}
	
	
	
}
